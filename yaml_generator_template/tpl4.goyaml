{{- range $metadaten := .Metadaten }}
{{- range $secrets := .Secrets }}
apiVersion: v1
kind: Secret
metadata:
  name: {{ $metadaten.Name}}
  namespace: {{ $metadaten.Namespace }}
  annotations: {{ $metadaten.Annotations }}
type: kubernetes.io/service-account-token
type: {{ $secrets.Type }}
data:
  {{ $secrets.Data }}
---
{{- end }}
{{- end }}
